{
  "version": 2,
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{if .Env.TitleHost}}{{.Env.TitleHost}}{{else}}{{.HostName}}{{end}} ({{.PWD}})",
  "palette": {
    "darkgrey": "#222222",
    "grey": "#666666"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "plain",
          "template": " {{ if .SSHSession }}\uf817 {{ end }}",
          "background": "yellow",
          "foreground": "p:darkgrey"
        },
        {
          "type": "os",
          "style": "plain",
          "template": " {{ if .WSL }}\uf68e {{ end }}",
          "background": "yellow",
          "foreground": "p:darkgrey"
        },
        {
          "type": "root",
          "style": "plain",
          "template": "\uf0e7",
          "background": "#ffcc88",
          "foreground": "p:darkgrey"
        },
        {
          "type": "os",
          "style": "plain",
          "template": " {{.Icon}} ",
          "background": "p:darkgrey",
          "foreground": "p:grey",
          "background_templates": [
            "{{if contains \"darwin\" .OS}}#FFFFFF{{end}}",
            "{{if contains \"ubuntu\" .OS}}#DD4814{{end}}",
            "{{if contains \"redhat\" .OS}}#EE0000{{end}}",
            "{{if contains \"fedora\" .OS}}#51A2DA{{end}}",
            "{{if contains \"windows\" .OS}}#00A2ED{{end}}"
          ],
          "foreground_templates": [
            "{{if contains \"darwin\" .OS}}p:grey{{end}}",
            "{{if contains \"ubuntu\" .OS}}#FFFFFF{{end}}",
            "{{if contains \"redhat\" .OS}}p:darkgrey{{end}}",
            "{{if contains \"fedora\" .OS}}#FFFFFF{{end}}",
            "{{if contains \"windows\" .OS}}#FFFFFF{{end}}"
          ],
          "properties": {
            "redhat": "\udb84\udd1b"
          }
        },
        {
          "type": "session",
          "style": "plain",
          "template": " {{ .UserName }}@{{ .HostName }} ",
          "background": "p:darkgrey",
          "foreground": "p:grey"
        },
        {
          "type": "path",
          "style": "plain",
          "template": " {{ .Path }} ",
          "background": "#444444",
          "foreground": "#cccccc",
          "properties": {
            "folder_icon": "\u2026",
            "style": "powerlevel",
            "max_width": 50
          }
        },
        {
          "type": "git",
          "style": "plain",
          "template": " {{ .HEAD }} ",
          "background": "#f14e32",
          "foreground": "#f0efe7",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf594 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          }
        },
        {
          "type": "dotnet",
          "style": "plain",
          "template": " \ue77f ",
          "background": "#7014eb",
          "foreground": "#ffffff",
          "properties": {
            "fetch_version": false
          }
        },
        {
          "type": "go",
          "style": "plain",
          "template": " \ufcd1 ",
          "background": "#7FD5EA",
          "foreground": "#ffffff",
          "properties": {
            "fetch_version": false
          }
        },
        {
          "type": "python",
          "style": "plain",
          "template": " \ue235 ",
          "background": "#906cff",
          "foreground": "#100e23",
          "properties": {
            "fetch_version": false
          }
        },
        {
          "type": "rust",
          "style": "plain",
          "template": " \ue7a8 ",
          "background": "#99908a",
          "foreground": "#193549",
          "properties": {
            "fetch_version": false
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "exit",
          "style": "plain",
          "template": "{{ if gt .Code 0 }} {{ .Code }} {{ end }}",
          "background": "#880000",
          "foreground": "#ff8888"
        },
        {
          "type": "executiontime",
          "style": "plain",
          "template": " {{ .FormattedMs }} ",
          "background": "#007ACC",
          "foreground": "#FFFFFF",
          "properties": {
            "style": "austin",
            "threshold": 321
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "template": "\u276f ",
          "foreground": "#007ACC"
        }
      ]
    }
  ]
}
